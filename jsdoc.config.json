{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 2,
  "source": {
    "include": ["./"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "/node_modules/"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "opts": {
    "destination": "./docs"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  }
}
